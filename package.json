{
  "name": "PollsClient",
  "version": "1.0.0",
  "description": "Polls client in AngularJS",
  "main": "server.js",
  "scripts": {
    "test": "karma start && protractor protractor.conf.js",
    "frontendtests": "karma start",
    "e2etests": "protractor protractor.conf.js",
    "postinstall": "jspm install && jspm bundle-sfx app/main build.js --minify --skip-source-maps && postcss --use autoprefixer build.css -o build.css"
  },
  "engines": {
    "node": "4.2.1",
    "npm": "3.3.9"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/XVincentX/pollsApiClient"
  },
  "keywords": [
    "Polls",
    "Angular"
  ],
  "author": "Vincenzo Chianese",
  "license": "MIT",
  "jspm": {
    "directories": {
      "lib": "."
    },
    "dependencies": {
      "angular": "npm:angular@^1.4.0",
      "angular-animate": "npm:angular-animate@^1.4.0",
      "angular-formly": "npm:angular-formly@^6.12.0-beta.1",
      "angular-hy-res": "npm:angular-hy-res@^0.0.27",
      "angular-mocks": "npm:angular-mocks@^1.4.3",
      "api-check": "npm:api-check@^7.2.4",
      "clean-css": "npm:clean-css@^3.3.4",
      "css": "github:systemjs/plugin-css@^0.1.9",
      "es6-promise": "npm:es6-promise@^2.3.0",
      "lodash": "npm:lodash@^3.8.0",
      "ng-promise-status": "npm:ng-promise-status@^0.1.2",
      "text": "github:systemjs/plugin-text@^0.0.2"
    },
    "devDependencies": {
      "babel": "npm:babel-core@^5.8.24",
      "babel-runtime": "npm:babel-runtime@^5.8.24",
      "core-js": "npm:core-js@^1.1.4"
    }
  },
  "devDependencies": {
    "chai": "^3.4.0",
    "chai-as-promised": "^5.1.0",
    "faker": "^3.0.1",
    "karma": "^0.13.14",
    "karma-chai": "^0.1.0",
    "karma-chai-as-promised": "^0.1.2",
    "karma-chrome-launcher": "^0.2.0",
    "karma-jspm": "^2.0.2",
    "karma-mocha": "^0.2.0",
    "karma-mocha-reporter": "^1.0.2",
    "mocha": "^2.3.0",
    "protractor": "^2.2.0"
  },
  "dependencies": {
    "autoprefixer": "^6.0.3",
    "compression": "^1.5.0",
    "consolidate": "^0.13.1",
    "cors": "^2.6.1",
    "ejs": "^2.3.1",
    "express": "^4.13.0",
    "jspm": "^0.16.2",
    "postcss-cli": "^2.3.1",
    "prerender-node": "^2.0.2"
  }
}
